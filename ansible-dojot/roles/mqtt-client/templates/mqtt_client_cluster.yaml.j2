apiVersion: apps/v1
kind: StatefulSet
metadata:
  name: mqtt-client
  namespace: dojot
spec:
  serviceName: "mqtt-client"
  selector:
    matchLabels:
      app: mqtt-client
  replicas: 4
  template:
    metadata:
      labels:
        app: mqtt-client
    spec:
      containers:
      - name: mqtt-client
        image: jonaphael/mqtt_client